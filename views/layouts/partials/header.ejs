<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if(!loggedin) { %>
        <a title="Click to log in" href="/account/login" id="login">My Account</a>
    <% } else { %>
        <a title="Click to main Page" href="/account/" id='welcome'>Welcome <%- locals.accountData.account_firstname %></a>
        <a title="Click to loggout" href="/account/loggout" id="loggout">Loggout</a>
    <% } %>
  </div>
</header>
